{
  "name": "teste-desenvolvedor-pleno",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/app.js",
    "server:prod": "node ./dist/app.js",
    "dev": "NODE_ENV=development npm-run-all server",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all build server:prod",
    "watch": "nodemon --watch src/ --exec babel-node src/app.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^5.7.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "nodemon": "^1.19.3",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0"
  }
}
